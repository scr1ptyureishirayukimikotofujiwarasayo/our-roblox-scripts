local Players=game:GetService("Players")local LocalPlayer=Players.LocalPlayer local UserInputService=game:GetService("UserInputService")local RunService=game:GetService("RunService")local TweenService=game:GetService("TweenService")local HttpService=game:GetService("HttpService")local CONFIG={WindowTitle="shirayukimikoto's mod menu v2",Colors={Primary=Color3.fromRGB(15,15,25),Secondary=Color3.fromRGB(25,25,40),Accent=Color3.fromRGB(88,101,242),Success=Color3.fromRGB(67,181,129),Danger=Color3.fromRGB(237,66,69),Warning=Color3.fromRGB(255,193,7),Text=Color3.fromRGB(255,255,255),TextDim=Color3.fromRGB(180,180,190)},Sizes={WindowWidth=380,WindowHeight=520,HeaderHeight=45,ButtonHeight=36,Spacing=8,CornerRadius=8},Keybind=Enum.KeyCode.RightControl}local function createCorner(instance,radius)local corner=Instance.new("UICorner")corner.CornerRadius=UDim.new(0,radius or CONFIG.Sizes.CornerRadius)corner.Parent=instance return corner end local function createPadding(instance,all)local padding=Instance.new("UIPadding")padding.PaddingTop=UDim.new(0,all)padding.PaddingBottom=UDim.new(0,all)padding.PaddingLeft=UDim.new(0,all)padding.PaddingRight=UDim.new(0,all)padding.Parent=instance return padding end local function safeParent(gui)local success,err=pcall(function()gui.Parent=game:GetService("CoreGui")end)if not success then local playerGui=LocalPlayer:FindFirstChild("PlayerGui")if playerGui then gui.Parent=playerGui end end end local screenGui=Instance.new("ScreenGui")screenGui.Name="ShirayukiModMenu"screenGui.ResetOnSpawn=false screenGui.ZIndexBehavior=Enum.ZIndexBehavior.Sibling safeParent(screenGui)local mainContainer=Instance.new("Frame")mainContainer.Name="MainContainer"mainContainer.Size=UDim2.new(0,CONFIG.Sizes.WindowWidth,0,CONFIG.Sizes.WindowHeight)mainContainer.Position=UDim2.new(0.5,0,0.5,0)mainContainer.AnchorPoint=Vector2.new(0.5,0.5)mainContainer.BackgroundColor3=CONFIG.Colors.Primary mainContainer.BorderSizePixel=0 mainContainer.Active=true mainContainer.Visible=true mainContainer.Parent=screenGui createCorner(mainContainer,CONFIG.Sizes.CornerRadius)local shadow=Instance.new("ImageLabel")shadow.Name="Shadow"shadow.BackgroundTransparency=1 shadow.Position=UDim2.new(0,-15,0,-15)shadow.Size=UDim2.new(1,30,1,30)shadow.ZIndex=0 shadow.Image="rbxassetid://59751741118"shadow.ImageColor3=Color3.fromRGB(0,0,0)shadow.ImageTransparency=0.5 shadow.ScaleType=Enum.ScaleType.Slice shadow.SliceCenter=Rect.new(10,10,118,118)shadow.Parent=mainContainer local header=Instance.new("Frame")header.Name="Header"header.Size=UDim2.new(1,0,0,CONFIG.Sizes.HeaderHeight)header.BackgroundColor3=CONFIG.Colors.Secondary header.BorderSizePixel=0 header.Parent=mainContainer createCorner(header,CONFIG.Sizes.CornerRadius)local headerTitle=Instance.new("TextLabel")headerTitle.Name="Title"headerTitle.Size=UDim2.new(1,-100,1,0)headerTitle.Position=UDim2.new(0,15,0,0)headerTitle.BackgroundTransparency=1 headerTitle.Text=CONFIG.WindowTitle headerTitle.TextColor3=CONFIG.Colors.Text headerTitle.Font=Enum.Font.GothamBold headerTitle.TextSize=16 headerTitle.TextXAlignment=Enum.TextXAlignment.Left headerTitle.Parent=header local closeBtn=Instance.new("TextButton")closeBtn.Name="CloseButton"closeBtn.Size=UDim2.new(0,35,0,35)closeBtn.Position=UDim2.new(1,-40,0.5,0)closeBtn.AnchorPoint=Vector2.new(0,0.5)closeBtn.BackgroundColor3=CONFIG.Colors.Danger closeBtn.BorderSizePixel=0 closeBtn.Text="×"closeBtn.TextColor3=CONFIG.Colors.Text closeBtn.Font=Enum.Font.GothamBold closeBtn.TextSize=20 closeBtn.Parent=header createCorner(closeBtn,6)closeBtn.MouseButton1Click:Connect(function()mainContainer.Visible=false end)local minimizeBtn=Instance.new("TextButton")minimizeBtn.Name="MinimizeButton"minimizeBtn.Size=UDim2.new(0,35,0,35)minimizeBtn.Position=UDim2.new(1,-80,0.5,0)minimizeBtn.AnchorPoint=Vector2.new(0,0.5)minimizeBtn.BackgroundColor3=CONFIG.Colors.Accent minimizeBtn.BorderSizePixel=0 minimizeBtn.Text="—"minimizeBtn.TextColor3=CONFIG.Colors.Text minimizeBtn.Font=Enum.Font.GothamBold minimizeBtn.TextSize=16 minimizeBtn.Parent=header createCorner(minimizeBtn,6)local isMinimized=false local originalSize=mainContainer.Size minimizeBtn.MouseButton1Click:Connect(function()isMinimized=not isMinimized local targetSize=isMinimized and UDim2.new(0,CONFIG.Sizes.WindowWidth,0,CONFIG.Sizes.HeaderHeight)or originalSize TweenService:Create(mainContainer,TweenInfo.new(0.3,Enum.EasingStyle.Quint),{Size=targetSize}):Play()end)local dragging=false local dragInput,dragStart,startPos header.InputBegan:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseButton1 then dragging=true dragStart=input.Position startPos=mainContainer.Position input.Changed:Connect(function()if input.UserInputState==Enum.UserInputState.End then dragging=false end end)end end)header.InputChanged:Connect(function(input)if input.UserInputType==Enum.UserInputType.MouseMovement then dragInput=input end end)UserInputService.InputChanged:Connect(function(input)if input==dragInput and dragging then local delta=input.Position-dragStart mainContainer.Position=UDim2.new(startPos.X.Scale,startPos.X.Offset+delta.X,startPos.Y.Scale,startPos.Y.Offset+delta.Y)end end)local contentContainer=Instance.new("ScrollingFrame")contentContainer.Name="Content"contentContainer.Size=UDim2.new(1,-20,1,-CONFIG.Sizes.HeaderHeight-20)contentContainer.Position=UDim2.new(0,10,0,CONFIG.Sizes.HeaderHeight+10)contentContainer.BackgroundTransparency=1 contentContainer.BorderSizePixel=0 contentContainer.ScrollBarThickness=4 contentContainer.ScrollBarImageColor3=CONFIG.Colors.Accent contentContainer.CanvasSize=UDim2.new(0,0,0,0)contentContainer.AutomaticCanvasSize=Enum.AutomaticSize.Y contentContainer.Parent=mainContainer local contentLayout=Instance.new("UIListLayout")contentLayout.SortOrder=Enum.SortOrder.LayoutOrder contentLayout.Padding=UDim.new(0,CONFIG.Sizes.Spacing)contentLayout.Parent=contentContainer local layoutOrder=0 local function getNextOrder()layoutOrder=layoutOrder+1 return layoutOrder end local function createSection(title,showHeader)local section=Instance.new("Frame")section.Name=title:gsub(" ","").."Section"section.BackgroundTransparency=1 section.Size=UDim2.new(1,0,0,0)section.AutomaticSize=Enum.AutomaticSize.Y section.LayoutOrder=getNextOrder()section.Parent=contentContainer local sectionLayout=Instance.new("UIListLayout")sectionLayout.SortOrder=Enum.SortOrder.LayoutOrder sectionLayout.Padding=UDim.new(0,6)sectionLayout.Parent=section if showHeader~=false then local headerFrame=Instance.new("Frame")headerFrame.Name="HeaderFrame"headerFrame.Size=UDim2.new(1,0,0,28)headerFrame.BackgroundColor3=CONFIG.Colors.Secondary headerFrame.BorderSizePixel=0 headerFrame.Parent=section createCorner(headerFrame,4)local headerLabel=Instance.new("TextLabel")headerLabel.Name="Header"headerLabel.Size=UDim2.new(1,0,1,0)headerLabel.BackgroundTransparency=1 headerLabel.Text=title headerLabel.TextColor3=CONFIG.Colors.Text headerLabel.Font=Enum.Font.GothamBold headerLabel.TextSize=14 headerLabel.TextXAlignment=Enum.TextXAlignment.Left headerLabel.Parent=headerFrame end return section end local function createButton(parent,text,color)local btn=Instance.new("TextButton")btn.Size=UDim2.new(1,0,0,CONFIG.Sizes.ButtonHeight)btn.BackgroundColor3=color or CONFIG.Colors.Secondary btn.BorderSizePixel=0 btn.Text=text btn.TextColor3=CONFIG.Colors.Text btn.Font=Enum.Font.Gotham btn.TextSize=13 btn.AutoButtonColor=false btn.LayoutOrder=getNextOrder()btn.Parent=parent createCorner(btn,6)btn.MouseEnter:Connect(function()TweenService:Create(btn,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(math.min((color or CONFIG.Colors.Secondary).R*255+20,255)/255,math.min((color or CONFIG.Colors.Secondary).G*255+20,255)/255,math.min((color or CONFIG.Colors.Secondary).B*255+20,255)/255)}):Play()end)btn.MouseLeave:Connect(function()TweenService:Create(btn,TweenInfo.new(0.2),{BackgroundColor3=color or CONFIG.Colors.Secondary}):Play()end)return btn end local function createToggle(parent,text,defaultState,callback)local state=defaultState or false local btn=createButton(parent,text..(state and" ON"or" OFF"),state and CONFIG.Colors.Success or CONFIG.Colors.Secondary)local function setState(newState)state=newState btn.Text=text..(state and" ON"or" OFF")btn.BackgroundColor3=state and CONFIG.Colors.Success or CONFIG.Colors.Secondary if callback then callback(state)end end btn.MouseButton1Click:Connect(function()setState(not state)end)return btn,function()return state end,setState end local function createTextBox(parent,placeholder)local box=Instance.new("TextBox")box.Size=UDim2.new(1,0,0,CONFIG.Sizes.ButtonHeight)box.BackgroundColor3=CONFIG.Colors.Secondary box.BorderSizePixel=0 box.Text=""box.PlaceholderText=placeholder box.TextColor3=CONFIG.Colors.Text box.PlaceholderColor3=CONFIG.Colors.TextDim box.Font=Enum.Font.Gotham box.TextSize=13 box.ClearTextOnFocus=false box.LayoutOrder=getNextOrder()box.Parent=parent createCorner(box,6)createPadding(box,8)return box end local function loadExternalScript(url,section,scriptName,callback)local statusLabel=Instance.new("TextLabel")statusLabel.Size=UDim2.new(1,0,0,24)statusLabel.BackgroundTransparency=1 statusLabel.Text="⏳ Loading "..scriptName.."..."statusLabel.TextColor3=CONFIG.Colors.Warning statusLabel.Font=Enum.Font.Gotham statusLabel.TextSize=12 statusLabel.TextXAlignment=Enum.TextXAlignment.Left statusLabel.LayoutOrder=getNextOrder()statusLabel.Parent=section task.spawn(function()local success,err=pcall(function()local source=game:HttpGet(url)local compiled,compileErr=loadstring(source,"@"..scriptName)if not compiled then error("Compile failed: "..tostring(compileErr))end local env=setmetatable({createButton=function(text,color)return createButton(section,text,color)end,createToggle=function(text,callback)return createToggle(section,text,false,callback)end,createTextBox=function(placeholder)return createTextBox(section,placeholder)end,MakeButton=function(text)return createButton(section,text)end,MakeToggle=function(p,text,callback)return createToggle(p or section,text,false,callback)end,MakeLabel=function(text)local lbl=Instance.new("TextLabel")lbl.Size=UDim2.new(1,0,0,24)lbl.BackgroundTransparency=1 lbl.Text=text lbl.TextColor3=CONFIG.Colors.Text lbl.Font=Enum.Font.GothamSemibold lbl.TextSize=13 lbl.TextXAlignment=Enum.TextXAlignment.Left lbl.LayoutOrder=getNextOrder()lbl.Parent=section return lbl end,MakeTextBox=function(text)return createTextBox(section,text)end,content=section,screenGui=screenGui,assignOrder=getNextOrder,safeParent=safeParent,game=game,workspace=workspace,LocalPlayer=LocalPlayer,Players=Players,UserInputService=UserInputService,RunService=RunService,TweenService=TweenService,HttpService=HttpService,Instance=Instance,Vector3=Vector3,Vector2=Vector2,CFrame=CFrame,Color3=Color3,UDim2=UDim2,UDim=UDim,Enum=Enum,print=print,warn=warn,error=error,pcall=pcall,task=task,wait=task.wait,spawn=task.spawn,math=math,table=table,string=string,tostring=tostring,tonumber=tonumber,pairs=pairs,ipairs=ipairs,next=next,type=type,_G=_G},{__index=getfenv()})setfenv(compiled,env)compiled()statusLabel.Text="✅ "..scriptName.." loaded!"statusLabel.TextColor3=CONFIG.Colors.Success if callback then callback(true)end end)if not success then statusLabel.Text="❌ "..scriptName.." failed to load"statusLabel.TextColor3=CONFIG.Colors.Danger warn("["..scriptName.."] Failed to load:",err)if callback then callback(false,err)end end end)end local movementSection=createSection("Movement Tools")local teleportSection=createSection("Teleport Tools")local btoolsSection=createSection("BTools")local tracerSection=createSection("Player Tracer")local emoteSection=createSection("Emote System")local dodgeSection=createSection("Side Dodge")local loggerSection=createSection("Client Logger")local dexSection=createSection("Dex Explorer")local topkSection=createSection("Topk3k Client")loadExternalScript("https://raw.githubusercontent.com/scr1ptyureishirayukimikotofujiwarasayo/our-roblox-scripts/main/shirayukimikoto_movement_tools.lua",movementSection,"MovementTools")local teleportGui=nil local teleportToolsLoaded=false local function getPlayerByName(name)for _,plr in ipairs(Players:GetPlayers())do if string.lower(plr.Name)==string.lower(name)then return plr end end return nil end local function createTeleportGUI()if teleportGui then teleportGui:Destroy()end teleportGui=Instance.new("Frame")teleportGui.Name="TeleportGUI"teleportGui.Size=UDim2.new(1,0,0,200)teleportGui.BackgroundTransparency=1 teleportGui.LayoutOrder=getNextOrder()teleportGui.Parent=teleportSection local layout=Instance.new("UIListLayout")layout.SortOrder=Enum.SortOrder.LayoutOrder layout.Padding=UDim.new(0,5)layout.Parent=teleportGui local playerNameBox=createTextBox(teleportGui,"Enter player name")local tpToBtn=createButton(teleportGui,"TP To Player",CONFIG.Colors.Accent)tpToBtn.MouseButton1Click:Connect(function()local target=getPlayerByName(playerNameBox.Text)if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")then LocalPlayer.Character:SetPrimaryPartCFrame(target.Character.HumanoidRootPart.CFrame+Vector3.new(0,0,3))end end)local bringBtn=createButton(teleportGui,"Bring Player",CONFIG.Colors.Accent)bringBtn.MouseButton1Click:Connect(function()local target=getPlayerByName(playerNameBox.Text)if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")then target.Character:SetPrimaryPartCFrame(LocalPlayer.Character.HumanoidRootPart.CFrame+Vector3.new(0,0,3))end end)local antiTpToggle,antiTpGet,antiTpSet=createToggle(teleportGui,"Anti-Teleport",false,function(state)if state then local char=LocalPlayer.Character if char and char:FindFirstChild("HumanoidRootPart")then char.HumanoidRootPart.Anchored=true end LocalPlayer.CharacterAdded:Connect(function(char)repeat task.wait()until char:FindFirstChild("HumanoidRootPart")char.HumanoidRootPart.Anchored=true end)else local char=LocalPlayer.Character if char and char:FindFirstChild("HumanoidRootPart")then char.HumanoidRootPart.Anchored=false end LocalPlayer.CharacterAdded:Connect(function(char)repeat task.wait()until char:FindFirstChild("HumanoidRootPart")char.HumanoidRootPart.Anchored=false end)end end)end local function loadTeleportTools(callback)local statusLabel=Instance.new("TextLabel")statusLabel.Size=UDim2.new(1,0,0,24)statusLabel.BackgroundTransparency=1 statusLabel.Text="⏳ Loading Teleport Tools..."statusLabel.TextColor3=CONFIG.Colors.Warning statusLabel.Font=Enum.Font.Gotham statusLabel.TextSize=12 statusLabel.TextXAlignment=Enum.TextXAlignment.Left statusLabel.LayoutOrder=getNextOrder()statusLabel.Parent=teleportSection task.spawn(function()statusLabel.Text="✅ Teleport Tools loaded!"statusLabel.TextColor3=CONFIG.Colors.Success createTeleportGUI()if callback then callback(true)end end)end local teleportBtn=createButton(teleportSection,"Load Teleport Tools",CONFIG.Colors.Accent)teleportBtn.MouseButton1Click:Connect(function()if teleportToolsLoaded then if teleportGui then teleportGui.Visible=not teleportGui.Visible end return end teleportBtn.Text="Loading..."teleportBtn.BackgroundColor3=CONFIG.Colors.Warning loadTeleportTools(function(success)if success then teleportToolsLoaded=true teleportBtn.Text="Toggle Teleport Tools"teleportBtn.BackgroundColor3=CONFIG.Colors.Success else teleportBtn.Text="Load Failed"teleportBtn.BackgroundColor3=CONFIG.Colors.Danger end end)end)local btoolsBtn=createButton(btoolsSection,"Load BTools GUI",CONFIG.Colors.Accent)btoolsBtn.MouseButton1Click:Connect(function()loadExternalScript("https://raw.githubusercontent.com/scr1ptyureishirayukimikotofujiwarasayo/our-roblox-scripts/main/shirayukimikoto_btools_gui.lua",btoolsSection,"BTools")end)local tracerEnabled=false local tracerConnections={}local function clearTracer(plr)if tracerConnections[plr]then for _,conn in ipairs(tracerConnections[plr])do conn:Disconnect()end tracerConnections[plr]=nil end if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")then local gui=plr.Character.HumanoidRootPart:FindFirstChild("TracerGui")if gui then gui:Destroy()end end end local function setupTracer(plr)if plr==LocalPlayer then return end local function attach(char)local hrp=char:WaitForChild("HumanoidRootPart",3)if not hrp then return end local billboard=Instance.new("BillboardGui")billboard.Name="TracerGui"billboard.Adornee=hrp billboard.Size=UDim2.new(0,200,0,40)billboard.StudsOffset=Vector3.new(0,3,0)billboard.AlwaysOnTop=true billboard.Parent=hrp local label=Instance.new("TextLabel")label.Size=UDim2.new(1,0,1,0)label.BackgroundTransparency=1 label.TextColor3=CONFIG.Colors.Text label.Font=Enum.Font.GothamBold label.TextSize=14 label.Text=plr.Name label.Parent=billboard local conn=RunService.Heartbeat:Connect(function()local myChar=LocalPlayer.Character local myRoot=myChar and myChar:FindFirstChild("HumanoidRootPart")if myRoot and hrp then local dist=(hrp.Position-myRoot.Position).Magnitude label.Text=plr.Name.." - "..math.floor(dist).." studs"end end)tracerConnections[plr]=tracerConnections[plr]or{}table.insert(tracerConnections[plr],conn)end if plr.Character then attach(plr.Character)end local c1=plr.CharacterAdded:Connect(attach)tracerConnections[plr]=tracerConnections[plr]or{}table.insert(tracerConnections[plr],c1)end createToggle(tracerSection,"Player Tracer",false,function(on)tracerEnabled=on if not on then for plr,_ in pairs(tracerConnections)do if plr~="_added"and plr~="_removed"then clearTracer(plr)end end tracerConnections={}return end for _,plr in ipairs(Players:GetPlayers())do setupTracer(plr)end if not tracerConnections["_added"]then tracerConnections["_added"]=Players.PlayerAdded:Connect(setupTracer)end if not tracerConnections["_removed"]then tracerConnections["_removed"]=Players.PlayerRemoving:Connect(clearTracer)end end)local emoteBtn=createButton(emoteSection,"Load Emote GUI",CONFIG.Colors.Accent)emoteBtn.MouseButton1Click:Connect(function()loadExternalScript("https://raw.githubusercontent.com/scr1ptyureishirayukimikotofujiwarasayo/our-roblox-scripts/main/emote_plus_gui.lua",emoteSection,"EmoteGUI")end)loadExternalScript("https://raw.githubusercontent.com/scr1ptyureishirayukimikotofujiwarasayo/our-roblox-scripts/main/shirayukimikoto_side_dodge.lua",dodgeSection,"SideDodge")local loggerBtn=createButton(loggerSection,"Load Client Logger",CONFIG.Colors.Accent)loggerBtn.MouseButton1Click:Connect(function()loadExternalScript("https://raw.githubusercontent.com/scr1ptyureishirayukimikotofujiwarasayo/our-roblox-scripts/main/fujiwarasayo_client_logger.lua",loggerSection,"ClientLogger")end)local dexBtn=createButton(dexSection,"Load Dex Explorer",CONFIG.Colors.Accent)dexBtn.MouseButton1Click:Connect(function()task.spawn(function()local ok,src=pcall(function()return game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/dex.lua")end)if ok and src then loadstring(src)()end end)end)local topkBtn=createButton(topkSection,"Load Topk3k Client",CONFIG.Colors.Accent)topkBtn.MouseButton1Click:Connect(function()loadExternalScript("https://raw.githubusercontent.com/scr1ptyureishirayukimikotofujiwarasayo/our-roblox-scripts/main/fujiwarasayo_topk3k.lua",topkSection,"Topk3k")end)UserInputService.InputBegan:Connect(function(input,gameProcessed)if not gameProcessed and input.KeyCode==CONFIG.Keybind then mainContainer.Visible=not mainContainer.Visible end end)print("✅ Fixed mod menu loaded successfully!")
