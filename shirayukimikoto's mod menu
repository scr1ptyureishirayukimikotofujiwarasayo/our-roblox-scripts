    local Exit = Instance.new("TextButton")
    Exit.Name = "Exit"
    Exit.Size = UDim2.new(0, 16, 0, 16)
    Exit.Position = UDim2.new(1, -18, 0, 2)
    Exit.BackgroundColor3 = Color3.fromRGB(150, 5, 44)
