        workspace.CurrentCamera.CameraSubject = LocalPlayer.Character.Humanoid
        workspace.CurrentCamera.CameraType = "Custom"
        LocalPlayer.CameraMode = Enum.CameraMode.Classic
    end)
    
    -- Appearance Section
    MakeTopkekLabel("[ Appearance ]")
    
    local nameInput = MakeTopkekTextBox(1/2, "Name")
    MakeTopkekButton(1/2, "Set Overhead Name", function()
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Head") then
            for _, v in pairs(LocalPlayer.Character:GetChildren()) do
                if v:IsA("Model") and v.Name == "NameTag" then
                    v:Destroy()
                end
            end
            
            local newName = Instance.new("Model", LocalPlayer.Character)
            newName.Name = "NameTag"
            local cl = LocalPlayer.Character.Head:Clone()
            cl.Parent = newName
            if cl:FindFirstChild("face") then
                cl.face:Destroy()
            end
            local hum = Instance.new("Humanoid", newName)
            hum.Name = "NameTag"
            hum.MaxHealth = 0
            hum.Health = 0
            local weld = Instance.new("Weld", cl)
            weld.Part0 = cl
            weld.Part1 = LocalPlayer.Character.Head
            LocalPlayer.Character.Head.Transparency = 1
            wait(0.5)
            cl.BrickColor = LocalPlayer.Character.Head.BrickColor
            
            local bb = Instance.new("BillboardGui", cl)
            bb.AlwaysOnTop = true
            bb.Size = UDim2.new(0, 100, 0, 50)
            bb.StudsOffset = Vector3.new(0, 1, 0)
            local tl = Instance.new("TextLabel", bb)
            tl.BackgroundTransparency = 1
            tl.Size = UDim2.new(1, 0, 1, 0)
            tl.Text = nameInput.Text
            tl.Font = Enum.Font.SourceSansBold
            tl.TextSize = 24
            tl.TextColor3 = Color3.new(1, 1, 1)
            tl.TextStrokeTransparency = 0
        end
    end)
    
    MakeTopkekButton(1/2, "Rainbow Name", function()
        LocalPlayer.Neutral = false
        spawn(function()
            while LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") do
                LocalPlayer.TeamColor = BrickColor.Random()
                wait(0.1)
            end
        end)
    end)
    
    MakeTopkekButton(1/2, "Clear Appearance", function()
        LocalPlayer:ClearCharacterAppearance()
    end)
    
