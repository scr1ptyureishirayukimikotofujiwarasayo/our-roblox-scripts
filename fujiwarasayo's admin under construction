    if args[1] then
        for i, v in pairs(findSinglePlayer(args[1])) do
            createINFO(v)
        end
    end
end

-- Age Command
Commands.age = function(args)
    if args[1] then
        for i, v in pairs(findPlayer(args[1])) do
            game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer(v.Name .. ' Account Age: ' .. v.AccountAge .. ' days!', 'All')
        end
    end
end

-- Invisible Command
Commands.invisible = function(args)
    local char = LP.Character
    if LP.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
        local clone = char.HumanoidRootPart:Clone()
        char.HumanoidRootPart:Destroy()
        clone.Parent = char
    else
        local clone = char.LowerTorso.Root:Clone()
        char.LowerTorso.Root:Destroy()
        clone.Parent = char.LowerTorso
    end
end

-- Walk Command
walkto = false
walkplr = ''
Commands.walk = function(args)
    if args[1] then
        for i, v in pairs(findSinglePlayer(args[1])) do
            walkplr = v
            walkto = true
            noclip = true
        end
    end
end

-- Unwalk Command
Commands.unwalk = function(args)
    walkto = false
    noclip = false
    LP.Character.Humanoid:MoveTo(LP.Character.HumanoidRootPart.Position)
end

-- Glitch Command
Commands.glitch = function(args)
    if hasTools() == false then
        Notify('warning', 'You need a tool in your backpack/inventory to use this command.', 8)
    else
        FEGodmode()
        for i, v in pairs(LP.Backpack:GetChildren()) do
            LP.Character.Humanoid:EquipTool(v)
        end
        if args[1] then
            for i, v in pairs(findSinglePlayer(args[1])) do
                local function tp(player, player2)
                    local char1, char2 = player.Character, player2.Character
                    if char1 and char2 then
                        char1.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
                    end
                end
                tp(game:GetService('Players')[v.Name], LP)
                wait(.2)
                tp(game:GetService('Players')[v.Name], LP)
                wait(.5)
                local bodyForce = Instance.new('BodyForce')
                bodyForce.Parent = LP.Character.HumanoidRootPart
                bodyForce.Name = 'Glitch'
                if args[2] then
                    bodyForce.Force = Vector3.new(args[2], 5000, 0)
                else
                    bodyForce.Force = Vector3.new(100000000, 5000, 0)
                end
                wait(6)
                bodyForce:Destroy()
            end
        end
    end
end

-- TP (Teleport) Command
Commands.tp = function(args)
    if args[1] then
        for i, v in pairs(findSinglePlayer(args[1])) do
            if v == LP then
                if args[2] then
                    for i, target in pairs(findSinglePlayer(args[2])) do
                        v.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame
                    end
                end
            else
                if hasTools() == false then
                    Notify('warning', 'You need a tool in your backpack/inventory to use this command.', 8)
                else
                    FEGodmode()
                    for i, v in pairs(LP.Backpack:GetChildren()) do
                        LP.Character.Humanoid:EquipTool(v)
                    end
                    if args[1] then
                        for i, first in pairs(findSinglePlayer(args[1])) do
                            if args[2] then
                                for i, second in pairs(findSinglePlayer(args[2])) do
                                    local function tp(player, player2)
                                        local char1, char2 = player.Character, player2.Character
                                        if char1 and char2 then
                                            char1.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
                                        end
                                    end
                                    local function getout(player, player2)
                                        local char1, char2 = player.Character, player2.Character
                                        if char1 and char2 then
                                            char1:MoveTo(char2.Head.Position)
                                        end
                                    end
                                    tp(LP, first)
                                    wait(.2)
                                    tp(LP, first)
                                    wait(.5)
                                    tp(LP, second)
                                    wait(.2)
                                    tp(LP, second)
                                    wait(.2)
                                    getout(LP, first)
                                end
                            end
                        end
                    end
                end
            end
        end
    end
end

-- Givetool Command
Commands.givetool = function(args)
    if args[1] then
        if args[2] then
            local selectedTool = ''
            for i, allTools in pairs(LP.Character:GetDescendants()) do
                if allTools:IsA('Tool') and string.lower(allTools.Name) == string.lower(args[2]) then
                    selectedTool = allTools
                else
                    for i, otherTools in pairs(LP.Backpack:GetDescendants()) do
                        if otherTools:IsA('Tool') and string.lower(otherTools.Name) == string.lower(args[2]) then
                            selectedTool = otherTools
                        end
                    end
                end
            end
            for i, v in pairs(findSinglePlayer(args[1])) do
                if selectedTool ~= '' then
                    selectedTool.Parent = v.Character
                end
            end
        else
            for i, plr in pairs(findSinglePlayer(args[1])) do
                for i, tool in pairs(LP.Character:GetDescendants()) do
                    if tool:IsA('Tool') then
                        tool.Parent = plr.Character
                    end
                end
            end
        end
    end
end

-- Givealltools Command
Commands.givealltools = function(args)
    LP.Character.Humanoid:UnequipTools()
    for i, plr in pairs(findSinglePlayer(args[1])) do
        for i, v in pairs(LP.Character:GetDescendants()) do
            if v:IsA('Tool') then
                v.Parent = plr.Character
            end
        end
        for i, tool in pairs(LP.Backpack:GetDescendants()) do
            if tool:IsA('Tool') then
                tool.Parent = plr.Character
            end
        end
    end
end

-- Blockhats Command
Commands.blockhats = function(args)
    for i, v in pairs(LP.Character:GetDescendants()) do
        if v:IsA('Accessory') or v:IsA('Hat') then
            for i, mesh in pairs(v:GetDescendants()) do
                if mesh.Name == 'Mesh' then
                    mesh:Destroy()
                end
            end
        end
    end
end

-- Blocktool Command
Commands.blocktool = function(args)
    for i, v in pairs(LP.Character:GetDescendants()) do
        if v:IsA('Tool') then
            for i, mesh in pairs(v:GetDescendants()) do
                if mesh.Name == 'Mesh' then
                    mesh:Destroy()
                end
            end
        end
    end
end
