-- Create command labels
function CreateCMDlabel(position, text)
    local labelHeight = 15
    local cmdLabel = Instance.new('TextLabel')
    cmdLabel.Name = 'cmdHere'
    cmdLabel.TextWrapped = true
    cmdLabel.Parent = ListofCMDS
    cmdLabel.BackgroundColor3 = Color3.new(1, 1, 1)
    cmdLabel.BackgroundTransparency = 1
    cmdLabel.BorderSizePixel = 0
    cmdLabel.Position = position
    cmdLabel.Size = UDim2.new(0, 1950, 0, labelHeight)
    cmdLabel.Font = Enum.Font.SourceSans
    cmdLabel.Text = text
    cmdLabel.TextWrapped = true
    cmdLabel.TextColor3 = Color3.new(.956863, .956863, .956863)
    cmdLabel.TextScaled = false
    cmdLabel.TextSize = 20
    cmdLabel.TextXAlignment = Enum.TextXAlignment.Left
end

-- Populate command list
for i, _cmds in pairs(searchCmds) do
    CreateCMDlabel(UDim2.new(0, 5, 0, 50 + (i * 15)), _cmds)
end
-- GUI Dragging Functionality for Commands GUI
local UserInputService = game:GetService('UserInputService')
local dragging
local dragInput
local dragStart
local startPos

local function updateCMDS(input)
    local delta = input.Position - dragStart
    local updateTime = .055
    local goals = {}
    goals.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    local moveTween = gsTween:Create(CMDSmain, TweenInfo.new(updateTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), goals)
    moveTween:Play()
end

cmdgui_topframe.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = CMDSmain.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

cmdgui_topframe.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

cmdgui_midframe.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = CMDSmain.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

cmdgui_midframe.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and dragging then
        updateCMDS(input)
    end
end)

-- Search Function Focus/Blur
cmdsgui_SearchFunction.Focused:Connect(function()
    cmdsgui_SearchFunction.TextTransparency = 0
    local textGoals = {}
    textGoals.TextColor3 = Color3.new(.0980392, .462745, .823529)
    textGoals.TextSize = 18
    textGoals.Position = UDim2.new(0, -70, 0, -15)
    
    local frameGoals = {}
    frameGoals.BackgroundColor3 = Color3.new(.0980392, .462745, .823529)
    
    local textTween = gsTween:Create(cmdsgui_searchDETAILTEXT, TweenInfo.new(.3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), textGoals)
    textTween:Play()
    
    local frameTween = gsTween:Create(cmdsgui_searchDETAILFRAME, TweenInfo.new(.3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), frameGoals)
    frameTween:Play()
end)

cmdsgui_SearchFunction.FocusLost:Connect(function(enterPressed)
    if not enterPressed then
        cmdsgui_SearchFunction.TextTransparency = 1
    else
        cmdsgui_SearchFunction.Text = ''
    end
    
    local textGoals = {}
    textGoals.TextColor3 = Color3.fromRGB(240, 240, 240)
    textGoals.TextSize = 30
    textGoals.Position = UDim2.new(0, 0, 0, 0)
    
    local frameGoals = {}
    frameGoals.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
    
    local textTween = gsTween:Create(cmdsgui_searchDETAILTEXT, TweenInfo.new(.3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), textGoals)
    textTween:Play()
    
    local frameTween = gsTween:Create(cmdsgui_searchDETAILFRAME, TweenInfo.new(.3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), frameGoals)
    frameTween:Play()
end)

-- Search Filter
cmdsgui_SearchFunction.Changed:Connect(function()
    local index = 0
    if cmdsgui_SearchFunction.Text ~= '' then
        for i, v in pairs(ListofCMDS:GetChildren()) do
            if v.Name == 'cmdHere' then
                if not string.find(v.Text, cmdsgui_SearchFunction.Text) then
                    v.Visible = false
                else
                    v.Visible = true
                    index = index + 1
                    v.Position = UDim2.new(0, 5, 0, 50 + (index * 15))
                end
            end
        end
    end
end)

-- Chat Command Handler
LP.Chatted:Connect(function(chat)
    run(chat)
end)

function run(message)
    if string.lower(string.sub(message, 2, 5)) == 'chat' then
        message = message
    elseif string.match(message, 'hotkey') and string.match(message, 'chat') then
        message = message
    else
        message = string.lower(message)
    end
    
    local prefix = string.sub(message, 1, 1)
    
    if prefix == commandPrefix then
        message = string.sub(message, 2)
        local args = {}
        
        for arg in string.gmatch(message, '[^%s]+') do
            table.insert(args, arg)
        end
        
        local cmdName = args[1]
        table.remove(args, 1)
        
        local cmdFunction = Commands[cmdName]
        
        if cmdFunction ~= nil then
            cmdFunction(args)
        end
    end
end

-- Command Bar GUI
local CommandBarGui = Instance.new('ScreenGui')
local CommandBarFrame = Instance.new('Frame')
local CommandBarText = Instance.new('TextBox')

CommandBarGui.Name = 'CommandBar'
CommandBarGui.Parent = gsCoreGui

CommandBarFrame.Name = 'CMDBAR'
CommandBarFrame.Parent = CommandBarGui
CommandBarFrame.BackgroundColor3 = Color3.new(.164706, .152941, .172549)
CommandBarFrame.BorderSizePixel = 0
CommandBarFrame.Position = UDim2.new(.025, 0, 1, 0)
CommandBarFrame.Size = UDim2.new(0, 270, 0, 35)

CommandBarText.Name = 'CMDBARText'
CommandBarText.Parent = CommandBarFrame
CommandBarText.BackgroundColor3 = Color3.new(.188235, .188235, .188235)
CommandBarText.BorderSizePixel = 0
CommandBarText.Position = UDim2.new(0, 5, 0, 5)
CommandBarText.Size = UDim2.new(0, 260, 0, 25)
CommandBarText.Font = Enum.Font.SourceSansLight
CommandBarText.Text = ''
CommandBarText.TextColor3 = Color3.new(.933333, .933333, .933333)
CommandBarText.TextScaled = true
CommandBarText.TextSize = 14
CommandBarText.TextWrapped = true

-- Command Bar Hotkey
Mouse.KeyDown:connect(function(Key)
    if Key == string.lower(commandPrefix) then
        CommandBarText:CaptureFocus()
        CommandBarFrame:TweenPosition(UDim2.new(.015, 0, .95, 0), 'Out', 'Elastic', .5, true)
    end
end)

-- Command Bar Focus Lost
CommandBarText.FocusLost:connect(function(enterPressed)
    CommandBarFrame:TweenPosition(UDim2.new(.015, 0, 1, 0), 'Out', 'Quad', .5, true)
    if enterPressed then
        local command = CommandBarText.Text
        CommandBarText.Text = ''
        run(commandPrefix .. command)
    end
end)

-- Command Suggestions GUI
local MatchFrame = Instance.new('Frame')
MatchFrame.Name = 'Match'
MatchFrame.Parent = CommandBarFrame
MatchFrame.BackgroundColor3 = Color3.new(.164706, .152941, .172549)
MatchFrame.BorderSizePixel = 0
MatchFrame.Position = UDim2.new(0, 0, -4, 0)
MatchFrame.Size = UDim2.new(1, 0, 4, 0)
MatchFrame.Visible = false

function CreateOption(Text)
    local Option = Instance.new('TextLabel')
    Option.Name = 'Option'
    Option.Parent = MatchFrame
    Option.BackgroundColor3 = Color3.new(1, 1, 1)
    Option.BackgroundTransparency = 1
    Option.Position = UDim2.new(-10, 0, 0, 0)
    Option.Size = UDim2.new(1, 0, 0, 20)
    Option.Font = Enum.Font.SourceSans
    Option.Text = Text
    Option.TextColor3 = Color3.new(.952941, .952941, .952941)
    Option.TextScaled = true
    Option.TextWrapped = true
end

-- Create command options
for i, cmdtext2 in pairs(CMDS) do
    CreateOption(cmdtext2)
end

-- Command Suggestions Filter
CommandBarText.Changed:Connect(function()
    if CommandBarText.Text ~= '' and CommandBarText.Text ~= commandPrefix then
        MatchFrame.Visible = true
        local index = 0
        for i, cmdtext in pairs(MatchFrame:GetChildren()) do
            if cmdtext.Name == 'Option' then
                if string.find(cmdtext.Text, CommandBarText.Text) then
                    cmdtext.Position = UDim2.new(0, 0, 0, 2 + (index * 20))
                    index = index + 1
                    if cmdtext.Position == UDim2.new(0, 0, 0, 142) then
                        cmdtext.Position = UDim2.new(-10, 0, 0, 0)
                        index = index - 1
                    end
                else
                    cmdtext.Position = UDim2.new(-10, 0, 0, 0)
                end
            end
        end
    else
        MatchFrame.Visible = false
    end
end)
