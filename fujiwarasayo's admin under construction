-- Fujiwara Sayo's Admin Script
-- Fixed and renamed version

trueSettings = {
    commandPrefix = ';',
    hotkeys = {},
    fchotkeymode = 'unfc'
}

-- Services
gsPlayers = game:GetService('Players')
gsWorkspace = game:GetService('Workspace')
gsLighting = game:GetService('Lighting')
gsReplicatedStorage = game:GetService('ReplicatedStorage')
gsCoreGui = game:GetService('CoreGui')
gsTween = game:GetService('TweenService')
gsHttp = game:GetService('HttpService')

-- Local Player
LP = gsPlayers.LocalPlayer
Mouse = LP:GetMouse()

defaultSettings = gsHttp:JSONEncode(trueSettings)

-- Save/Load Functions
function CreateSave()
    writefile('FujiwaraSayo.txt', defaultSettings)
    wait(.5)
    local fileContent = readfile('FujiwaraSayo.txt')
    local decodedSettings = gsHttp:JSONDecode(fileContent)
    commandPrefix = decodedSettings.commandPrefix
    hotkeys = decodedSettings.hotkeys
    fchotkeymode = decodedSettings.fchotkeymode
end

function fullUpdate()
    local updatedSettings = {
        commandPrefix = commandPrefix,
        hotkeys = hotkeys,
        fchotkeymode = fchotkeymode
    }
    local encodedSettings = gsHttp:JSONEncode(updatedSettings)
    wait(.2)
    writefile('FujiwaraSayo.txt', encodedSettings)
end

-- Initialize Settings
if writefile ~= nil then
    function builder()
        local fileContent = readfile('FujiwaraSayo.txt')
        if fileContent == nil then
            return false
        else
            return true
        end
    end
    
    local success, error = pcall(builder)
    
    if success == true then
        function reader()
            local fileContent = readfile('FujiwaraSayo.txt')
            local decodedSettings = gsHttp:JSONDecode(fileContent)
            commandPrefix = decodedSettings.commandPrefix
            hotkeys = decodedSettings.hotkeys
            if decodedSettings.fchotkeymode == nil then
                fchotkeymode = 'unfc'
                fullUpdate()
            else
                fchotkeymode = decodedSettings.fchotkeymode
            end
        end
        reader()
    elseif success == false then
        CreateSave()
    end
else
    commandPrefix = ';'
    hotkeys = {}
    fchotkeymode = 'unfc'
end

-- Current Settings
CurrentGravity = gsWorkspace.Gravity
CurrentWalkspeed = LP.Character.Humanoid.WalkSpeed
CurrentJumppower = LP.Character.Humanoid.JumpPower
CurrentHipheight = LP.Character.Humanoid.HipHeight
CurrentNormal = LP.DevCameraOcclusionMode

-- Camera FOV Lock
gsWorkspace.Camera.Changed:Connect(function()
    gsWorkspace.Camera.FieldOfView = 70
end)

-- View Function
function view(plr)
    if plr.Character.Humanoid ~= nil then
        gsWorkspace.CurrentCamera.CameraSubject = plr.Character.Humanoid
    else
        gsWorkspace.CurrentCamera.CameraSubject = plr.Character.Head
    end
end

-- Workspace Lock/Unlock
function unlockWS()
    for i, part in pairs(gsWorkspace:GetDescendants()) do
        if part:IsA('Part') then
            part.Locked = false
        end
    end
end

function lockWS()
    for i, part in pairs(gsWorkspace:GetDescendants()) do
        if part:IsA('Part') then
            part.Locked = true
        end
    end
end

-- FE Godmode
function FEGodmode()
    local isCurrentCamera = false
    if gsWorkspace.CurrentCamera.CameraSubject == LP.Character.Humanoid or 
       gsWorkspace.CurrentCamera.CameraSubject == LP.Character then
        isCurrentCamera = true
    end
    
    LP.Character.Humanoid.Name = 1
    local clone = LP.Character['1']:Clone()
    clone.Parent = LP.Character
    clone.Name = 'Humanoid'
    wait(.1)
    LP.Character['1']:Destroy()
    
    if isCurrentCamera then
        game:GetService('Workspace').CurrentCamera.CameraSubject = LP.Character
    end
    
    LP.Character.Animate.Disabled = true
    wait(.1)
    LP.Character.Animate.Disabled = false
    LP.Character.Humanoid.DisplayDistanceType = 'None'
end

-- Rocket Propulsion
function RocketPropulsion(maxthrust, maxspeed, thrustp, targetplr, name)
    local rocketProp = Instance.new('RocketPropulsion')
    rocketProp.Parent = LP.Character.HumanoidRootPart
    rocketProp.CartoonFactor = 1
    rocketProp.MaxThrust = maxthrust
    rocketProp.MaxSpeed = maxspeed
    rocketProp.ThrustP = thrustp
    rocketProp.Name = name
    rocketProp.Target = targetplr.Character.HumanoidRootPart
    rocketProp:Fire()
end
-- Notification and Intro Functions
function createIntro(style, message, length)
    if gsCoreGui:FindFirstChild('Notification') then
        gsCoreGui:FindFirstChild('Notification'):Destroy()
    end
    
    local infoIcon = 'http://www.roblox.com/asset/?id=1281284684'
    local warningIcon = 'http://www.roblox.com/asset/?id=1281286925'
    
    if style == 'info' then
        style = infoIcon
    elseif style == 'warning' then
        style = warningIcon
    end
    
    local ScreenGui = Instance.new('ScreenGui')
    local Frame = Instance.new('Frame')
    local TextLabel = Instance.new('TextLabel')
    local ImageLabel = Instance.new('ImageLabel')
    
    ScreenGui.Name = 'Notification'
    ScreenGui.Parent = gsCoreGui
    
    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.new(.164706, .164706, .164706)
    Frame.BackgroundTransparency = .20000000298023
    Frame.BorderSizePixel = 0
    Frame.Position = UDim2.new(0, 0, -.2, 0)
    Frame.Size = UDim2.new(1, 0, 0, 30)
    
    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
    TextLabel.BackgroundTransparency = 1
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.Font = Enum.Font.SourceSansLight
    TextLabel.Text = message
    TextLabel.TextColor3 = Color3.new(.905882, .905882, .905882)
    TextLabel.TextScaled = true
    TextLabel.TextSize = 14
    TextLabel.TextWrapped = true
    
    ImageLabel.Parent = Frame
    ImageLabel.BackgroundTransparency = 1
    ImageLabel.Size = UDim2.new(0, 50, 0, 50)
    ImageLabel.Position = UDim2.new(.1, 0, 0, 0)
    ImageLabel.Image = style
    
    local IntroGui = Instance.new('ScreenGui')
    local IntroFrame = Instance.new('Frame')
    local IntroImage = Instance.new('ImageLabel')
    
    IntroGui.Name = 'Intro'
    IntroGui.Parent = gsCoreGui
    
    IntroFrame.Parent = IntroGui
    IntroFrame.BackgroundTransparency = 1
    IntroFrame.BorderSizePixel = 0
    IntroFrame.Position = UDim2.new(0, 0, -.2, 0)
    IntroFrame.Size = UDim2.new(1, 0, 0, 30)
    
    IntroImage.Parent = Frame
    IntroImage.BackgroundTransparency = 1
    IntroImage.AnchorPoint = Vector2.new(.5, 0)
    IntroImage.Size = UDim2.new(0, 240, 0, 120)
    IntroImage.Position = UDim2.new(.5, 0, 0, 0)
    IntroImage.Image = 'http://www.roblox.com/asset/?id=1795472522'
    
    IntroFrame:TweenPosition(UDim2.new(0, 0, 0, 200), 'Out', 'Quad', 1.5)
    Frame:TweenPosition(UDim2.new(0, 0, 0, 0), 'Out', 'Quad', 1.5)
    
    wait(length)
    pcall(function()
        Frame:TweenPosition(UDim2.new(0, 0, -1.5, 0), 'Out', 'Quad', 3)
        IntroFrame:TweenPosition(UDim2.new(0, 0, -1.5, 0), 'Out', 'Quad', 3)
    end)
    wait(3.01)
    IntroGui:Destroy()
    ScreenGui:Destroy()
end

function Notify(style, message, length)
    if gsCoreGui:FindFirstChild('Notification') then
        gsCoreGui:FindFirstChild('Notification'):Destroy()
    end
    
    local infoIcon = 'http://www.roblox.com/asset/?id=1281284684'
    local warningIcon = 'http://www.roblox.com/asset/?id=1281286925'
    
    if style == 'info' then
        style = infoIcon
    elseif style == 'warning' then
        style = warningIcon
    end
    
    local ScreenGui = Instance.new('ScreenGui')
    local Frame = Instance.new('Frame')
    local TextLabel = Instance.new('TextLabel')
    local ImageLabel = Instance.new('ImageLabel')
    
    ScreenGui.Name = 'Notification'
    ScreenGui.Parent = gsCoreGui
    
    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.new(.164706, .164706, .164706)
    Frame.BackgroundTransparency = .20000000298023
    Frame.BorderSizePixel = 0
    Frame.Position = UDim2.new(0, 0, -.2, 0)
    Frame.Size = UDim2.new(1, 0, 0, 30)
    
    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
    TextLabel.BackgroundTransparency = 1
    TextLabel.Size = UDim2.new(1, 0, 1, 0)
    TextLabel.Font = Enum.Font.SourceSansLight
    TextLabel.Text = message
    TextLabel.TextColor3 = Color3.new(.905882, .905882, .905882)
    TextLabel.TextScaled = true
    TextLabel.TextSize = 14
    TextLabel.TextWrapped = true
    
    ImageLabel.Parent = Frame
    ImageLabel.BackgroundTransparency = 1
    ImageLabel.Size = UDim2.new(0, 50, 0, 50)
    ImageLabel.Position = UDim2.new(.1, 0, 0, 0)
    ImageLabel.Image = style
    
    Frame:TweenPosition(UDim2.new(0, 0, 0, 0), 'Out', 'Quad', 1.5)
    wait(length)
    pcall(function()
        Frame:TweenPosition(UDim2.new(0, 0, -1.5, 0), 'Out', 'Quad', 3)
    end)
    wait(3.01)
    ScreenGui:Destroy()
end

-- Check if player has tools
function hasTools()
    local hasCharacterTools = false
    local hasBackpackTools = false
    
    for i, v in pairs(LP.Character:GetDescendants()) do
        if v:IsA('Tool') then
            if v ~= nil then
                hasCharacterTools = true
            else
                hasCharacterTools = false
            end
        end
    end
    
    for i, k in pairs(LP.Backpack:GetDescendants()) do
        if k:IsA('Tool') then
            if k ~= nil then
                hasBackpackTools = true
            else
                hasBackpackTools = false
            end
        end
    end
    
    return hasCharacterTools or hasBackpackTools
end

-- Compliment Messages
Compliments = {
    ' is the coolest person in this server!',
    ', I really like your avatar!',
    ', I really want to be your friend!',
    ' is truly amazing. Truly!',
    ' is incredible!',
    ', you are my favourite here!!',
    ', I am complimenting you right now at this very moment.',
    ' you are really awesome',
    ' when will you be my friend!?',
    ' is such a great person',
    ' is a fantastic person!'
}

function complimentplr(player)
    local playerName = player.Name
    game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer(
        playerName .. Compliments[math.random(1, #Compliments)], 'All'
    )
end
-- Create Player Info GUI
function createINFO(player)
    local ScreenGui = Instance.new('ScreenGui')
    local MainFrame = Instance.new('Frame')
    local TopFrame = Instance.new('Frame')
    local CloseButton = Instance.new('TextButton')
    local MiddleFrame = Instance.new('Frame')
    local PlayerNameLabel = Instance.new('TextLabel')
    local BottomFrame = Instance.new('Frame')
    local PlayerAvatar = Instance.new('ImageLabel')
    local PlayerAccAge = Instance.new('TextLabel')
    local PlayerId = Instance.new('TextLabel')
    local PlayerOs = Instance.new('TextLabel')
    local PlayerMembership = Instance.new('TextLabel')
    local Shadow1 = Instance.new('Frame')
    local Shadow2 = Instance.new('Frame')
    
    ScreenGui.Name = 'InfoGUIv2'
    ScreenGui.Parent = gsCoreGui
    
    MainFrame.Parent = ScreenGui
    MainFrame.BackgroundColor3 = Color3.new(0, 0, 0)
    MainFrame.BackgroundTransparency = 1
    MainFrame.BorderColor3 = Color3.new(0, 0, 0)
    MainFrame.ClipsDescendants = true
    MainFrame.Position = UDim2.new(.45, 0, 1, 0)
    MainFrame.Size = UDim2.new(0, 265, 0, 302)
    MainFrame.ZIndex = -1
    
    TopFrame.Parent = MainFrame
    TopFrame.BackgroundColor3 = Color3.new(.290196, 0, .447059)
    TopFrame.BorderSizePixel = 0
    TopFrame.Size = UDim2.new(0, 260, 0, 20)
    
    CloseButton.Name = 'infoguiCLOSE'
    CloseButton.Parent = TopFrame
    CloseButton.BackgroundColor3 = Color3.new(1, 1, 1)
    CloseButton.BackgroundTransparency = 1
    CloseButton.BorderSizePixel = 0
    CloseButton.Position = UDim2.new(0, 230, 0, 0)
    CloseButton.Size = UDim2.new(0, 30, 0, 20)
    CloseButton.Font = Enum.Font.SourceSansBold
    CloseButton.Text = 'X'
    CloseButton.TextColor3 = Color3.new(.992157, .992157, .992157)
    CloseButton.TextSize = 20
    
    MiddleFrame.Parent = MainFrame
    MiddleFrame.BackgroundColor3 = Color3.new(.482353, .121569, .635294)
    MiddleFrame.BorderSizePixel = 0
    MiddleFrame.Position = UDim2.new(0, 0, 0, 20)
    MiddleFrame.Size = UDim2.new(0, 260, 0, 40)
    
    PlayerNameLabel.Name = 'playerName'
    PlayerNameLabel.Parent = MiddleFrame
    PlayerNameLabel.BackgroundColor3 = Color3.new(1, 1, 1)
    PlayerNameLabel.BackgroundTransparency = 1
    PlayerNameLabel.Position = UDim2.new(0, 10, 0, 5)
    PlayerNameLabel.Size = UDim2.new(0, 240, 0, 30)
    PlayerNameLabel.Font = Enum.Font.SourceSansLight
    PlayerNameLabel.Text = player.Name
    PlayerNameLabel.TextColor3 = Color3.new(.988235, .988235, .988235)
    PlayerNameLabel.TextScaled = true
    PlayerNameLabel.TextSize = 14
    PlayerNameLabel.TextWrapped = true
    
    BottomFrame.Parent = MainFrame
    BottomFrame.BackgroundColor3 = Color3.new(.956863, .956863, .956863)
    BottomFrame.BorderSizePixel = 0
    BottomFrame.Position = UDim2.new(0, 0, 0, 60)
    BottomFrame.Size = UDim2.new(0, 260, 0, 237)
    
    PlayerAvatar.Name = 'playerAvatar'
    PlayerAvatar.Parent = BottomFrame
    PlayerAvatar.BackgroundColor3 = Color3.new(1, 1, 1)
    PlayerAvatar.Position = UDim2.new(0, 85, 0, 10)
    PlayerAvatar.Size = UDim2.new(0, 85, 0, 85)
    PlayerAvatar.Image = 'http://www.roblox.com/Thumbs/Avatar.ashx?x=150&y=150&Format=Png&username=' .. player.Name
    
    PlayerAccAge.Name = 'playerAccAge'
    PlayerAccAge.Parent = BottomFrame
    PlayerAccAge.BackgroundColor3 = Color3.new(1, 1, 1)
    PlayerAccAge.BackgroundTransparency = 1
    PlayerAccAge.Position = UDim2.new(0, 5, 0, 101)
    PlayerAccAge.Size = UDim2.new(0, 250, 0, 30)
    PlayerAccAge.Font = Enum.Font.SourceSans
    PlayerAccAge.Text = 'Account Age: ' .. player.AccountAge
    PlayerAccAge.TextColor3 = Color3.new(.0784314, .0784314, .0784314)
    PlayerAccAge.TextScaled = true
    PlayerAccAge.TextSize = 14
    PlayerAccAge.TextWrapped = true
    
    PlayerId.Name = 'playerId'
    PlayerId.Parent = BottomFrame
    PlayerId.BackgroundColor3 = Color3.new(1, 1, 1)
    PlayerId.BackgroundTransparency = 1
    PlayerId.Position = UDim2.new(0, 5, 0, 131)
    PlayerId.Size = UDim2.new(0, 250, 0, 30)
    PlayerId.Font = Enum.Font.SourceSans
    PlayerId.Text = 'Account ID: ' .. player.UserId
    PlayerId.TextColor3 = Color3.new(.0784314, .0784314, .0784314)
    PlayerId.TextScaled = true
    PlayerId.TextSize = 14
    PlayerId.TextWrapped = true
    
    PlayerOs.Name = 'playerOs'
    PlayerOs.Parent = BottomFrame
    PlayerOs.BackgroundColor3 = Color3.new(1, 1, 1)
    PlayerOs.BackgroundTransparency = 1
    PlayerOs.Position = UDim2.new(0, 5, 0, 161)
    PlayerOs.Size = UDim2.new(0, 250, 0, 30)
    PlayerOs.Font = Enum.Font.SourceSansLight
    PlayerOs.Text = 'Player OS: ' .. player.OsPlatform
    PlayerOs.TextColor3 = Color3.new(.0784314, .0784314, .0784314)
    PlayerOs.TextScaled = true
    PlayerOs.TextSize = 14
    PlayerOs.TextWrapped = true
    
    PlayerMembership.Name = 'playerMembership'
    PlayerMembership.Parent = BottomFrame
    PlayerMembership.BackgroundColor3 = Color3.new(1, 1, 1)
    PlayerMembership.BackgroundTransparency = 1
    PlayerMembership.Position = UDim2.new(0, 5, 0, 191)
    PlayerMembership.Size = UDim2.new(0, 250, 0, 30)
    PlayerMembership.Font = Enum.Font.SourceSansLight
    
    if player.MembershipType == Enum.MembershipType.None then
        PlayerMembership.Text = "No builder's club."
    elseif player.MembershipType == Enum.MembershipType.BuildersClub then
        PlayerMembership.Text = "Builder's club!"
    elseif player.MembershipType == Enum.MembershipType.TurboBuildersClub then
        PlayerMembership.Text = "Turbo Builder's club!"
    elseif player.MembershipType == Enum.MembershipType.OutrageousBuildersClub then
        PlayerMembership.Text = "Outrageous Builder's club!"
    end
    
    PlayerMembership.TextColor3 = Color3.new(.0784314, .0784314, .0784314)
    PlayerMembership.TextScaled = true
    PlayerMembership.TextSize = 14
    PlayerMembership.TextWrapped = true
    
    Shadow1.Parent = MainFrame
    Shadow1.BackgroundColor3 = Color3.new(0, 0, 0)
    Shadow1.BackgroundTransparency = .69999998807907
    Shadow1.BorderColor3 = Color3.new(0, 0, 0)
    Shadow1.BorderSizePixel = 0
    Shadow1.ClipsDescendants = true
    Shadow1.Position = UDim2.new(0, 10, 0, 10)
    Shadow1.Selectable = true
    Shadow1.Size = UDim2.new(0, 255, 0, 292)
    Shadow1.ZIndex = -1
    
    Shadow2.Parent = MainFrame
    Shadow2.BackgroundColor3 = Color3.new(0, 0, 0)
    Shadow2.BackgroundTransparency = .69999998807907
    Shadow2.BorderColor3 = Color3.new(0, 0, 0)
    Shadow2.BorderSizePixel = 0
    Shadow2.ClipsDescendants = true
    Shadow2.Position = UDim2.new(0, 8, 0, 8)
    Shadow2.Selectable = true
    Shadow2.Size = UDim2.new(0, 255, 0, 292)
    Shadow2.ZIndex = -1
    
    -- Tween animations
    local tweenGoals = {}
    tweenGoals.Size = UDim2.new(0, 0, 0, 0)
    
    local tweenPosition = {}
    tweenPosition.Position = UDim2.new(.45, 0, .45, 0)
    
    local closeTween = gsTween:Create(MainFrame, TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), tweenGoals)
    local openTween = gsTween:Create(MainFrame, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), tweenPosition)
    
    CloseButton.MouseButton1Click:Connect(function()
        closeTween:Play()
        MainFrame:TweenPosition((MainFrame.Position + UDim2.new(0, 265/2, 0, 302/2)), 'InOut', 'Sine', 2)
        wait(2.01)
        MainFrame:Destroy()
    end)
    
    openTween:Play()
    
    -- Dragging functionality
    local UserInputService = game:GetService('UserInputService')
    local dragging
    local dragInput
    local dragStart
    local startPos
    
    local function update(input)
        local delta = input.Position - dragStart
        local updateTime = .055
        local goals = {}
        goals.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        local moveTween = gsTween:Create(MainFrame, TweenInfo.new(updateTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), goals)
        moveTween:Play()
    end
    
    MainFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    MainFrame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging and MainFrame.Size == UDim2.new(0, 265, 0, 302) then
            update(input)
        end
    end)
end

function clientSided()
    Notify('info', 'This command is for the client (you) only, no one else can see!', 6)
end
