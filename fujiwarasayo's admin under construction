    if args[1] then
        for i, v in pairs(findPlayer(args[1])) do
            trlplr = v
        end
        if args[2] then
            trlnum = args[2]
        else
            trlnum = 5
        end
        trailing = true
    else
        Notify('warning', 'No player selected to trail! Use: ' .. commandPrefix .. 'trail player', 4)
    end
end

-- Untrail Command
Commands.untrail = function(args)
    trailing = false
end

