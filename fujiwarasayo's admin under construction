    LP.Character['Left Leg']:Destroy()
    LP.Character['Right Leg']:Destroy()
end

-- Headless Command
Commands.headless = function(args)
    local clone = LP.Character.Humanoid:Clone()
    LP.Character.Humanoid:Destroy()
    wait(.2)
    LP.Character.Head.CanCollide = false
    for i, v in pairs(LP.Character:GetDescendants()) do
        if string.sub(v.Name, 1, 4) == 'Neck' then
            v:Destroy()
        end
    end
    wait(.2)
    clone.Name = 'Humanoid'
    clone.Parent = LP.Character
    wait(.1)
    game:GetService('Workspace').CurrentCamera.CameraSubject = LP.Character
    LP.Character.Animate:Destroy()
end

-- Antikick Command
antiremotes = false
Commands.antikick = function(args)
    if args[1] then
        if args[1] == 'on' then
            antiremotes = true
            wait(.2)
            for i, v in pairs(LP.Character:GetChildren()) do
                if string.find(string.lower(v.Name), 'exploit') and v:IsA('LocalScript') then
                    v.Disabled = true
                end
            end
            Notify('warning', 'This command disables all remotes in case they are kick remotes, may break game.', 8)
            Notify('info', 'Does not prevent serverside kicks, use ' .. commandPrefix .. 'antikick off to turn off.', 8)
        elseif args[1] == 'off' then
            antiremotes = false
            Notify('warning', 'Remote anti-kick turned off.', 8)
        end
    end
end
-- Block Remote Command
blockedremotes = {}
Commands.blockremote = function(args)
    local service = ''
    if args[1] then
        local remoteName = string.lower(tostring(args[1]))
        if args[2] then
            local serviceInput = string.lower(tostring(args[2]))
            if serviceInput == 'workspace' then
                service = 'Workspace'
            elseif serviceInput == 'replicatedstorage' then
                service = 'ReplicatedStorage'
            elseif serviceInput == 'players' then
                service = 'Players'
            elseif serviceInput == 'lighting' then
                service = 'Lighting'
            elseif serviceInput == 'startergui' then
                service = 'StarterGui'
            elseif serviceInput == 'starterpack' then
                service = 'StarterPack'
            elseif serviceInput == 'starterplayer' then
                service = 'StarterPlayer'
            else
                service = 'ReplicatedStorage'
            end
        else
            service = 'ReplicatedStorage'
        end
        for i, getRemote in pairs(game:GetService(service):GetDescendants()) do
            if string.lower(getRemote.Name) == remoteName then
                table.insert(blockedremotes, getRemote.Name)
            end
        end
    end
    Notify('warning', 'If this command does not work, make sure you type remote name/service fully correct.', 8)
end

-- Remotespy Command
spyingremotes = false
Commands.remotespy = function(args)
    if args[1] then
        if args[1] == 'on' then
            spyingremotes = true
            Notify('info', 'Remotespy turned on.', 4)
        elseif args[1] == 'off' then
            spyingremotes = false
            Notify('info', 'Remotespy turned off.', 4)
        end
    end
end

-- Bang Command
Commands.bang = function(args)
    if args[1] then
        for i, v in pairs(findSinglePlayer(args[1])) do
            if v ~= nil then
                following = true
                flwplr = v
                flwnum = -1
                local anim = Instance.new('Animation')
                anim.AnimationId = 'rbxassetid://148840371'
                bangTrack = LP.Character.Humanoid:LoadAnimation(anim)
                if args[2] then
                    bangTrack:Play(.1, 1, args[2])
                else
                    bangTrack:Play(.1, 1, 1)
                end
            end
        end
    else
        Notify('warning', 'No player selected to follow! Use: ' .. commandPrefix .. 'bang player', 4)
    end
end

-- Unbang Command
Commands.unbang = function(args)
    following = false
    bangTrack:Stop()
end

-- Spam Commands
spamdelay = 1
spamtext = 'Spam'
spamming = false

Commands.spam = function(args)
    if args[1] then
        spamtext = args[1]
        spamming = true
    end
end

Commands.spamdelay = function(args)
    if args[1] then
        spamdelay = args[1]
    end
end

spawn(function()
    while wait(spamdelay) do
        if spamming then
            game:GetService('ReplicatedStorage').DefaultChatSystemChatEvents.SayMessageRequest:FireServer(spamtext, 'All')
        end
    end
end)

Commands.unspam = function(args)
    spamming = false
end

