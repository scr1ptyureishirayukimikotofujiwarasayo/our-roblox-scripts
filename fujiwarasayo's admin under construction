Commands.unlockws = function(args)
    unlockWS()
    Notify('info', 'Workspace unlocked.', 4)
end

-- Noclip Command
Commands.noclip = function(args)
    noclip = true
    Notify('info', 'Noclip enabled.', 4)
end

-- Clip Command
Commands.clip = function(args)
    noclip = false
    Notify('info', 'Noclip disabled.', 4)
end

-- Follow Command
Commands.follow = function(args)
    if args[1] then
        for i, v in pairs(findPlayer(args[1])) do
            flwplr = v
        end
        if args[2] then
            flwnum = args[2]
        else
            flwnum = -5
        end
        following = true
    else
        Notify('warning', 'No player selected to follow! Use: ' .. commandPrefix .. 'follow player', 4)
    end
end

-- Unfollow Command
Commands.unfollow = function(args)
    following = false
end

-- Fling Command
Commands.fling = function(args)
    if args[1] then
        for i, v in pairs(findSinglePlayer(args[1])) do
            if v ~= LP then
                view(v)
                pcall(function()
                    LP.Character.HumanoidRootPart.Fling:Destroy()
                end)
                if not args[2] then
                    RocketPropulsion(800000, 1000, 400000, v, 'Fling')
                else
                    RocketPropulsion(args[2], 1500, 400000, v, 'Fling')
                end
                if noclip ~= true then
                    flingnoclip = true
                    noclip = true
                end
            end
        end
    else
        Notify('warning', 'No player selected to fling! Use: ' .. commandPrefix .. 'fling player', 4)
    end
end

-- Unfling Command
Commands.unfling = function(args)
    view(LP)
    pcall(function()
        if LP.Character.HumanoidRootPart.Fling then
            for i, v in pairs(LP.Character:GetDescendants()) do
                if v.Name == 'Fling' and v:IsA('RocketPropulsion') then
                    v:Destroy()
                end
            end
        end
    end)
    if flingnoclip == true then
        noclip = false
        flingnoclip = false
    end
end

